	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug local mode",
			"request": "launch",
			"type": "dart",
			"args": [
				"--dart-define-from-file=env.local.json"
			]
		},
		{
			"name": "Debug staging mode",
			"request": "launch",
			"type": "dart",
			"args": [
				"--dart-define-from-file=env.staging.json"
			]
		},
		{
			"name": "Debug production mode",
			"request": "launch",
			"type": "dart",
			"args": [
				"--dart-define-from-file=env.prod.json"
			]
		},
		{
			"name": "Launch (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			"name": "Launch (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release"
		}
	]
}